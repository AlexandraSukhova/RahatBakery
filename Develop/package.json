{
  "name": "rahatbakery",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "check": "prettier --check './**/*.{html,scss,ts}'",
    "format": "prettier --write './**/*.{html,scss,ts}'",
    "lint": "stylelint '**/*.scss'",
    "lintfix": "stylelint '**/*.scss' --fix",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "sass": "^1.83.0",
    "sass-embedded": "^1.83.0",
    "typescript": "~5.6.2",
    "vite": "^6.0.1"
  },
  "dependencies": {
    "gh-pages": "^6.2.0",
    "prettier": "^3.4.2",
    "stylelint": "^16.11.0"
  }
}
